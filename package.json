{
	"name": "obsidian-historica",
	"version": "0.0.1",
	"description": "Auto generate timeline of content in the note or across the vault",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"yarn run css\" \"yarn run esbuild\" ",
		"css": "tailwindcss -i function/src.css -o styles.css --watch",
		"esbuild": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/js-yaml": "^4.0.9",
		"@types/lodash": "^4.14.202",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "^10.4.17",
		"builtin-modules": "3.3.0",
		"concurrently": "^8.2.2",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"postcss": "^8.4.35",
		"tailwindcss": "^3.4.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"chrono-node": "^2.7.5",
		"js-yaml": "^4.1.0",
		"marked": "^12.0.0",
		"toml": "^3.0.0",
		"wink-eng-lite-web-model": "^1.5.2",
		"wink-nlp": "^1.14.3"
	}
}
